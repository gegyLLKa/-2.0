# Добро пожаловать в программу с рецептами тортов!
# Выберите номер торта, чтобы получить его рецепт.

while True:
    print("1. Шоколадный торт")
    print("2. Ванильный торт")
    print("3. Торт "Тирамису"")
    print("4. Ореховый торт")
    print("5. Фруктовый чизкейк")
    print("6. Красный бархатный торт")
    print("7. Ягодный торт")
    print("8. Морковный торт")
    print("9. Медовик")
    print("10. Торт "Наполеон"")
    print("11. Выход")

    choice = input("Выберите номер торта или введите '11' для выхода: ")

    if choice == "1":
        print("Рецепт шоколадного торта:")
        # Рецепт шоколадного торта
        
    elif choice == "2":
        print("Рецепт ванильного торта:")
        # Рецепт ванильного торта

    elif choice == "3":
        print("Рецепт торта 'Тирамису':")
        # Рецепт торта 'Тирамису'
        
    elif choice == "4":
        print("Рецепт орехового торта:")
        # Рецепт орехового торта
        
    elif choice == "5":
        print("Рецепт фруктового чизкейка:")
        # Рецепт фруктового чизкейка
        
    elif choice == "6":
        print("Рецепт красного бархатного торта:")
        # Рецепт красного бархатного торта
        
    elif choice == "7":
        print("Рецепт ягодного торта:")
        # Рецепт ягодного торта
        
    elif choice == "8":
        print("Рецепт морковного торта:")
        # Рецепт морковного торта
        
    elif choice == "9":
        print("Рецепт медовика:")
        # Рецепт медовика
    
    elif choice == "10":
        print("Рецепт торта 'Наполеон':")
        # Рецепт торта 'Наполеон'
        
    elif choice == "11":
        print("До свидания!")
        break

    else:
        print("Пожалуйста, выберите номер торта от 1 до 11.")
